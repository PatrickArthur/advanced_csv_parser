      <td><%= line.csv_id %></td>
      <td><%= line.first_name %></td>
      <td><%= line.last_name %></td>
      <td><%= line.company %></td>
      <td><%= line.email %></td>
      <td><%= line.address1 %></td>
      <td><%= line.address2 %></td>
      <td><%= line.zip %></td>
      <td><%= line.city %></td>
      <td><%= line.state_long %></td>
      <td><%= line.state %></td>
      <td><%= line.phone %></td>
    </tr>
  <% end %>
<% end %>
</table>

